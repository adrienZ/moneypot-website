{
  "name": "app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "packageManager": "pnpm@9.0.2",
  "engines": {
    "node": "20.12.2"
  },
  "scripts": {
    "prebuild": "pnpm lint",
    "postbuild": "pnpm ip:dl-dbs",
    "build": "nuxt build",
    "dev": "BASE_URL=http://localhost:3000 nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "BASE_URL=https://moneypot-website.onrender.com nuxt preview",
    "postinstalll": "nuxt prepare",
    "db:generate": "pnpm exec drizzle-kit generate",
    "db:push": "pnpm exec drizzle-kit push",
    "db:studio": "pnpm exec drizzle-kit studio",
    "ip:dl-dbs": "node -e \"import('geolite2-redist').then(geolite => geolite.downloadDbs())\"",
    "lint": "eslint .",
    "test": "pnpm exec playwright test",
    "test:ui": "pnpm test -- --ui",
    "typecheck": "nuxt prepare && nuxt typecheck"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/image": "^1.5.0",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.14.2",
    "@types/qrcode": "^1.5.5",
    "@types/ua-parser-js": "^0.7.39",
    "@vue-email/nuxt": "^0.8.12",
    "@vue/eslint-config-prettier": "^9.0.0",
    "drizzle-kit": "^0.22.7",
    "eslint": "^9.4.0",
    "nuxt": "^3.12.1",
    "nuxt-primevue": "^0.3.1",
    "prettier": "^3.3.2",
    "uploadthing": "^6.12.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3",
    "vue-tsc": "^1.8.22"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.1.43",
    "@nuxt/ui": "^2.16.0",
    "arctic": "^1.9.1",
    "drizzle-orm": "^0.31.2",
    "geolite2-redist": "^3.0.4",
    "lucia": "3.2.0",
    "maxmind": "^4.3.20",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "primevue": "^3.52.0",
    "qrcode": "^1.5.3",
    "quill": "^2.0.2",
    "radix-vue": "^1.8.3",
    "resend": "^3.3.0",
    "stripe": "^15.10.0",
    "twemoji": "^14.0.2",
    "ua-parser-js": "^1.0.38",
    "vue-email": "^0.8.11",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  }
}
